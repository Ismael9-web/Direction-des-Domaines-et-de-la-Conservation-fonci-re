      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="AttributionLot" name="Attribution du Lot">
      <bpmn:extensionElements>
        <zeebe:formDefinition formKey="attribution-lot-form" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="EnregistrerDonnees" name="Enregistrer les Données">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="register-data" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="CreerNotification" name="Créer la Notification">
      <bpmn:extensionElements>
        <zeebe:formDefinition formKey="create-notification-form" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="AppelPostgreSQL" name="Appel à PostgreSQL et MinIO">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="database-storage" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="EncaisserNotification" name="Encaisser la Notification">
      <bpmn:extensionElements>
        <zeebe:formDefinition formKey="collect-notification-form" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="GenererRecu" name="Générer le Reçu">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="generate-receipt" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_1" name="Fin du processus">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="AttributionLot" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="AttributionLot" targetRef="EnregistrerDonnees" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="EnregistrerDonnees" targetRef="CreerNotification" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="CreerNotification" targetRef="AppelPostgreSQL" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="AppelPostgreSQL" targetRef="EncaisserNotification" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="EncaisserNotification" targetRef="GenererRecu" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="GenererRecu" targetRef="EndEvent_1" />
